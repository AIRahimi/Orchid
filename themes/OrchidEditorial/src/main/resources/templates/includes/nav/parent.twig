{% block parent %}
    {% import 'macros/nav.twig' as _nav_ %}
    <li>
        <span class="opener">{{ parent.name }}</span>
        <ul>
            {% for child in children %}
                {% block children %}
                    {{ _nav_.createLink(child) }}
                {% endblock %}
            {% endfor %}
        </ul>
    </li>
{% endblock %}
