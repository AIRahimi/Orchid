{% import 'macros/type.twig' as _type_ %}
{% import 'macros/comment.twig' as _comment_ %}
{% import 'macros/anchor.twig' as _anchor_ %}

<tr>
    <td>
        <code><a href="#{{- _anchor_.constructorAnchor(constructor) -}}">{{- classDoc.info.name -}}</a>({{- _type_.formatParameterList(constructor.parameters) -}})</code>
        <br><br>
        <span>{{- _comment_.formatCommentPreview(constructor.comment, options) -}}</span>
    </td>
</tr>
