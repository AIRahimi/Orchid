{% extends [page.layout, concat('layouts/', page.layout, '.twig'), "layouts/geopattern2.twig"] %}

{% block pageNav %}

{% endblock %}

{% block jumbotron %}
    <h1 class="home-title">{{ page.title }}</h1>
{% endblock %}

{% block mainContent %}

    <section class="bs-docs-section">
        {% set blogPages = index.posts.blog | walk('url') | sortPosts | limit(postArchive.pageSize, postArchive.page) %}
        <ul>
            {% for blogPage in blogPages %}
                <li><h1><a href="{{- blogPage.url -}}">{{- blogPage.name -}}</a></h1></li>
            {% endfor %}
        </ul>
    </section>

{% endblock %}
